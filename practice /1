//TEMPLATE USING OODP

#include <iostream>
using namespace std ;

template<typename T>
T concat (T n1 , T n2) 
{
     return (n1 + n2);
}


int main() {
  cout << concat(5,6) << endl;
  cout << concat(2.5 , 7.5) << endl;
  cout << concat ('R', 'A')<<endl;
  cout << concat (string("Ram"),string("Kumar")) << endl;
  
  
    
}
